   "source": [
    "from collections import deque\n",
    "N, K = map(int, input().split())\n",
    "table = deque([i for i in range(1,N+1)])\n",
    "ans = []\n",
    "while len(table) != 0:\n",
    "    for _ in range(K-1):\n",
    "        table.append(table.popleft())\n",
    "    ans.append(str(table.popleft()))\n",
    "print('<' + ', '.join(ans) + '>')\n",
    "    \n",
    "    "
   ]
  }
