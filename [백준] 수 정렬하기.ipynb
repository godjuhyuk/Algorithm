   "source": [
    "# bubble sort\n",
    "arr = [5,4,3,2,1]\n",
    "for _ in range(len(arr)-1):\n",
    "    for i in range(len(arr) - 1):\n",
    "        if arr[i] > arr[i+1]:\n",
    "            temp = arr[i]\n",
    "            arr[i] = arr[i+1]\n",
    "            arr[i+1] = temp\n",
    "\n",
    "for i in arr:\n",
    "    print(i)"
   ]
