    "N=int(input())\n",
    "min_ = 2000\n",
    "for x in range(N//3 + 1):\n",
    "    for y in range(N//5 + 1):\n",
    "        if 3*x + 5*y == N:\n",
    "            if min_ >= x+y:\n",
    "                min_ = x+y\n",
    "\n",
    "print(min_) if min_ != 2000 else print(-1)  "
   ]
