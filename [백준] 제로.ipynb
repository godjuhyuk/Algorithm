    "import sys\n",
    "num_stack = []\n",
    "for _ in range(int(input())):\n",
    "    n = int(sys.stdin.readline().strip())\n",
    "    if n == 0:\n",
    "        num_stack.pop()\n",
    "    else:    \n",
    "        num_stack.append(n)\n",
    "sys.stdout.write(str(sum(num_stack)))"
   ]
